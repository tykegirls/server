<p id="notice"><%= notice %></p>

<h1>Saving Exps</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @saving_exps.each do |saving_exp| %>
      <tr>
        <td><%= link_to 'Show', saving_exp %></td>
        <td><%= link_to 'Edit', edit_saving_exp_path(saving_exp) %></td>
        <td><%= link_to 'Destroy', saving_exp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

