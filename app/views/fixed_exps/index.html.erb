<p id="notice"><%= notice %></p>

<h1>Fixed Exps</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fixed_exps.each do |fixed_exp| %>
      <tr>
        <td><%= link_to 'Show', fixed_exp %></td>
        <td><%= link_to 'Edit', edit_fixed_exp_path(fixed_exp) %></td>
        <td><%= link_to 'Destroy', fixed_exp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fixed Exp', new_fixed_exp_path %>
